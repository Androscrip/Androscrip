function Main2()
  FX = "Main"
 mkjll={
    "â¬†ï¸ Traspasar",
    "ðŸ” Recolectar",
    "ðŸŒ Mundo", 
    "ðŸ”® Magia",
    "ðŸ‘” Armario",
