function alas1()
FX = "alas1"
  if #winslist == 0 then
    gg.toast('failed to load coord list!')
    return;
  end
  getmap()
  if #crarrayalas == 0 then
    alas.map = X
    alas.level = 0
    for k,v in ipairs(winslist) do
      if v.map == X then
        table.insert(crarrayalas,v)
      end
    end
    if #crarrayalas == 0 then
      jy=gg.choice({'ðŸ”™ Exit',
    'ðŸ•¯Velas',
    'ðŸŽ…ðŸ»EspÃ­ritu',
    'â©Teletransportarse',
      },nil,'ID: '..X..'â—No data on this map')
      if jy == 1 then
        gg.setVisible(false)
        crarrayalas = {}
        alas.level = 0
        alas.map = ''
        alas.enable = false
        Main()
      end
      if jy == 2 then
        Farm2()
      end
      if jy == 3 then
        spiritu()
      end
      if jy == 4 then
        Telelist()
      end
    end
  end
  if X ~= alas.map or #crarrayalas == 0 then
    crarrayalas = {}
    alas.level = 0
    alas.map = ''
    return;
  end
  jy=gg.choice({
    'âž¡ï¸Siguiente Punto',
    'â¬…ï¸Punto anterior',
    'â¬†ï¸Romper Muro',
    'ðŸ•¯Velas',
    'ðŸŽ…ðŸ»EspÃ­ritu',
    'â©Teletransportarse',
    'â¸Lista de coordenadas',
    'ðŸ—ºï¸Cambiar mapa',
    "ðŸ’•Cambiar mapa con amigos",
    'ðŸ”™ Exit',
  },nil,''..X..'Stars : ' .. alas.level .. '/' .. #crarrayalas)
  if jy == 1 then
    gg.setVisible(true)
    if alas.level >= #crarrayalas then
      alas.level = #crarrayalas - 1
    end
    alas.level = alas.level + 1
    Teleport(crarrayalas[alas.level].x,crarrayalas[alas.level].y,crarrayalas[alas.level].z)
  end
  if jy == 2 then
    gg.setVisible(true)
    if alas.level <= 1 then
      alas.level = 2
    end
    alas.level = alas.level - 1
    Teleport(crarrayalas[alas.level].x,crarrayalas[alas.level].y,crarrayalas[alas.level].z)
  end
  if jy == 3 then
    pmove(settings.warpdis )
  end
  if jy == 4 then
  crset.enable = true
  spirit.enable = false
    Farm2()
  end
  if jy == 5 then
    spiritu()
  end
  if jy == 6 then
    Telelist()
  end
  if jy == 7 then
    xvw = {}
    for i,v in ipairs(crarrayalas) do
      table.insert(xvw,'['..i..'] '..math.floor(v.x)..' / '..math.floor(v.y)..' / '..math.floor(v.z))
    end
    xve = gg.choice(xvw,nil,'')
    if xve == nil then
      return;
      else
      alas.level = xve
      Teleport(crarrayalas[alas.level].x,crarrayalas[alas.level].y,crarrayalas[alas.level].z)
    end
  end
  
    if jy == 8 then
    changemap() 
end 
if jy == 9 then
teleportamigos()
end
