revertflygravity = flygrvt[1].value

end
if flygravity == true then
          flygravity = false
          flygrvt[1].value = revertflygravity
          gg.setValues(flygrvt)
          gg.toast('flygravity deactivated')
        else
          flygravity = true
          flygrvt[1].value = '6'
          gg.setValues(flygrvt)
          gg.toast('flygravity activated')
        end
    elseif menu == 26 then  
    Main()
 
  end
  end

