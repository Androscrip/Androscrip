
function Farm2()
FX = "Farm2"
  if #candle == 0 then
    gg.toast('failed to load coord list!')
    return;
  end
  getmap()
  if X == "Night" then setadd(CambiarCapa,4,2219120716)      end
  if #crarray == 0 then
    crset.map = X
    crset.level = 0
    for k,v in ipairs(candle) do
      if v.map == X then
        table.insert(crarray,v)
      end
    end
    if #crarray == 0 then
      jy=gg.choice({'ðŸ”™ Exit',
    'ðŸ¦‡Estrellas',
    'ðŸŽ…ðŸ»EspÃ­ritu',
