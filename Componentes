function setting()
detectar_android() 
FX = "setting"
 menu = gg.choice({
    "ðŸš«ðŸ“¡ Quitar Chat del Lobby, Produce Lentitud al Abrir",
    "â¬†ï¸ Distancia de movimiento   [".. settings.warpdis  .."] Metros ",
    "ðŸ–¼ï¸ Fotogramas   [".. getadd(FPS,gg.TYPE_FLOAT) .."] Por Segundos ",
"ðŸŒ¡ï¸ Velocidad  [".. getadd(speedbase,gg.TYPE_FLOAT) .."]",
    "ðŸ”˜ BotÃ³n GG",
    "ðŸ’¾ Guardar",
    "ðŸ”™ Exit",
  }, nil, "AJUSTE âš™")
    if menu == 1 then
    if settings.mains == false then settings.mains = true gg.toast("Encendido") return else settings.mains = false gg.toast("Apagado") end
    elseif menu == 2 then
    settings.warpdis = inputnum(nn)
    elseif menu == 3 then  
    settings.FPS = inputnum(nn)
    if not fps then gg.toast("Fps No Encontrado Reinicia el Juego â—") else setadd(fps,gg.TYPE_FLOAT,settings.FPS,true) end
    elseif menu == 4 then  
local nspeed = inputnum(nn)
if not nspeed then else
setadd(speedbase,gg.TYPE_FLOAT,nspeed,true) gg.toast("Done") end
elseif menu == 5 then  
if settings.ui == true then settings.ui = false gg.toast("OFF") else settings.ui = true gg.toast("Encendido") end 

    elseif menu == 6 then  
    savedata()
    gg.toast("ConfiguraciÃ³n Guardada âœ”ï¸")
    elseif menu == 7 then  
