FX = "Word"     
getmap()

  menu7 = gg.choice({
    "ðŸ“ UbicaciÃ³n",
    "ðŸ•¹ Dibujar ",
    "âž¡ï¸ Teletransporte",
    "ðŸ—ºï¸ Cambiar Mapa",
    "ðŸ’•Cambiar mapa con amigos",
    "â¬‡ ï¸Guardar esta ubicaciÃ³n",
    "â¬†ï¸ Cargar ubicaciÃ³n guardada    " ..math.floor(px).. " "..math.floor(py).." "..math.floor(pz).."",
    "â Avanzar",
    "â« Mover hacia arriba",
    "â¬ Mover hacia abajo",
    "ðŸ”‘ Desbloquear",
    "ðŸ’¨ Remover Pared De Viento",
    "ðŸ’¨ Despejar Viento",
    "ðŸ”™ Exit",
